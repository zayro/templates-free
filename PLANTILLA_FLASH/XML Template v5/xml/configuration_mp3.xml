<?xml version="1.0" ?>

<configuration>
		
	<!-- url mp3 -->
	<URL_MP3_LIST>xml/mp3.xml</URL_MP3_LIST>
	
	<!-- color list  -->
	<MP3_NUMBER_COLOR_ROL>0xF4D200</MP3_NUMBER_COLOR_ROL>
	<MP3_NUMBER_COLOR_OUT>0xCCCCCC</MP3_NUMBER_COLOR_OUT>
	
		
	<!-- /////Volume   -->
	<MP3_VOLUME_COLOR_BACKGROUND>0x727272</MP3_VOLUME_COLOR_BACKGROUND>
	<MP3_VOLUME_COLOR_CURRENT>0xAEAEAE</MP3_VOLUME_COLOR_CURRENT>
	<MP3_VOLUME_COLOR_CIRCLE>0xFFFFFF</MP3_VOLUME_COLOR_CIRCLE>	
	<MP3_VOLUME_GLOW_COLOR_SCROLL>0x999999</MP3_VOLUME_GLOW_COLOR_SCROLL>
	
	
	<!-- Speaker  -->/
	<MP3_SPEAKER_COLOR_ROL>0x727272</MP3_SPEAKER_COLOR_ROL>
	<MP3_SPEAKER_COLOR_OUT>0xB6B6B6</MP3_SPEAKER_COLOR_OUT>
	
	<!--  background  player -->
	<MP3_BACKGROUND_COLOR>0x333333</MP3_BACKGROUND_COLOR>
	
	<!-- progress -->
	<MP3_BACKGROUND_PROGRESS>0x727272</MP3_BACKGROUND_PROGRESS>	
	<MP3_CURRENT_PROGRESS>0xAEAEAE</MP3_CURRENT_PROGRESS>
	<MP3_PRELOADER_COLOR>0x727272</MP3_PRELOADER_COLOR>	
	<MP3_PROGRESS_COLOR_CIRCLE>0xFFFFFF</MP3_PROGRESS_COLOR_CIRCLE>	
	<MP3_PROGRESS_GLOW_COLOR_SCROLL>0x999999</MP3_PROGRESS_GLOW_COLOR_SCROLL>
	
	<!-- time view -->
	<MP3_TIME_COLOR>0xB9B9B9</MP3_TIME_COLOR>	
		
	<!-- BUFFER COLOR -->
	<MP3_BUFFER_COLOR>0xAEAEAE</MP3_BUFFER_COLOR>
	
	<!-- align title mp3 ==> (R OR L)  -->
	<MP3_ALIGN_TITLE>R</MP3_ALIGN_TITLE>	
	
	<!-- position X  -->
	<MP3_POSITION_X>0</MP3_POSITION_X>
	
	<!-- position Y -->
	<MP3_POSITION_Y>0</MP3_POSITION_Y>
	
	<!-- width -->
	<MP3_WIDTH>800</MP3_WIDTH>	
	
	<!-- AUTO PLAY   (true/falase) -->
	<MP3_AUTO_PLAY>true</MP3_AUTO_PLAY>
	
	<!-- volume first -->
	<MP3_VOLUME_FIRST>70</MP3_VOLUME_FIRST>	
				
	<!-- circle visible (true/false) -->  
	<MP3_CIRCLE_VISIBLE>false</MP3_CIRCLE_VISIBLE>	
	
	
		
</configuration>
