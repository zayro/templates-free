<?xml version="1.0" encoding="utf-8"?>


<configuration>

	<!-- color background tools -->
	<FLV_BACKGROUND_TOOLS>0x313331</FLV_BACKGROUND_TOOLS>
	<!-- color background player -->	
	<FLV_BACKGROUND_PLAYER>0x000000</FLV_BACKGROUND_PLAYER>
	<!-- ALPHA background player -->	
	<FLV_BACKGROUND_PLAYER_ALPHA>85</FLV_BACKGROUND_PLAYER_ALPHA>
	<!-- color symbol onrollOver (speaker)-->	
	<FLV_COLOR_SYMBOL_BUTTON_ROLLOVER>0x727272</FLV_COLOR_SYMBOL_BUTTON_ROLLOVER>
	<!-- color symbol onRollOut (only speaker) -->	
	<FLV_COLOR_SYMBOL_BUTTON_ROLLOUT>0xB6B6B6</FLV_COLOR_SYMBOL_BUTTON_ROLLOUT>
	<!-- color time -->
	<FLV_COLOR_NUMBER>0XB9B9B9</FLV_COLOR_NUMBER>
		<!-- color background preloader circle -->
	<FLV_COLOR_BACKGROUND_BUFFER>0x313331</FLV_COLOR_BACKGROUND_BUFFER>	
		
	<!-- SOUND  -->
	<!-- color line sound -->
	<FLV_COLOR_LINE_SOUND>0x727272</FLV_COLOR_LINE_SOUND>
	<!-- color line current -->
	<FLV_CURRENT_SOUND_LINE>0xAEAEAE</FLV_CURRENT_SOUND_LINE>	
	
	
	<!-- PROGRESS  move-->
	<!-- color line progress -->
	<FLV_COLOR_LINE_PROGRESS>0x666666</FLV_COLOR_LINE_PROGRESS>
	<!-- color line progress preloader -->
	<FLV_COLOR_LINE_PROGRESS_PRELOADER>0x727272</FLV_COLOR_LINE_PROGRESS_PRELOADER>
	<!-- COLOR CURRENT PROGRESS -->
	<FLV_CURRENT_PROGRESS_LINE>0xAEAEAE</FLV_CURRENT_PROGRESS_LINE>	
	
	
	<!-- PARAMETERS	-->
	<NEW_WIDTH>640</NEW_WIDTH>	
	<NEW_HEIGHT>520</NEW_HEIGHT>
	<FLV_AUTO_PLAY>true</FLV_AUTO_PLAY>	
	<FLV_AUTO_REPLAY>false</FLV_AUTO_REPLAY>
	<FLV_BUFFER_SIZE>6</FLV_BUFFER_SIZE>
	<FULL_SCRENN_BUTTON>true</FULL_SCRENN_BUTTON>	
	<FLV_SOUND_ROLL_SAMPLE>false</FLV_SOUND_ROLL_SAMPLE>

	
		
</configuration>


