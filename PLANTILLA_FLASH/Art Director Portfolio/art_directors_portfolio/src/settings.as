stop();// Variables that get set from XML filevar LOGO_NAME:String;var LOGO_NAME_COLOR:Number;var LOGO_TAGLINE:String;var LOGO_TAGLINE_COLOR:Number;var USE_OWN_LOGO:Boolean = false;var YOUR_LOGO_FILE:String;var BASIC_TEXT_COLOR:Number;var FOOTER_TEXT:String;var FOOTER_TEXT_COLOR:Number;var EMAIL_ADDRESS:String;var EMAIL_TEXT:String;var NAV_COLOR:Number;var NAV_ROLLOVER_COLOR:Number;var NAV_SELECTED_COLOR:Number;var EMAIL_COLOR:Number;var EMAIL_ROLLOVER_COLOR:Number;var BUTTON_COLOR:Number;var BUTTON_ROLLOVER_COLOR:Number;var PRELOADER_COLOR:Number;var BACKGROUND_COLOR:Number;var BACKGROUND_GRADIENT_COLOR1:Number;var BACKGROUND_GRADIENT_COLOR2:Number;var MIDDLE_COLOR:Number;var MIDDLE_GRADIENT_COLOR1:Number;var MIDDLE_GRADIENT_COLOR2:Number;var BORDER_COLOR:Number;var THUMB_BACKGROUND_COLOR:Number;var THUMB_BORDER_COLOR:Number;var THUMB_ROLLOVER_COLOR:Number;var THUMB_TEXT_COLOR:Number;var THUMB_TEXT_ROLLOVER_COLOR:Number;var NEW_ICON_COLOR:Number;var DISPLAY_GRADIENT_STRIPES:Boolean = true;var DEFAULT_PAGE_TITLE:String;var SELECTED_WORK_PAGE_TITLE:String;var ABOUT_PAGE_TITLE:String;var CONTACT_PAGE_TITLE:String;var RIGHT_CLICK_COPYRIGHT:String;var PADDING_BOTTOM:Number;var settings_xml:XML = new XML();settings_xml.ignoreWhite = true;settings_xml.onLoad = function(success:Boolean) {	if(success) {		parseSettingsXML(this);		XMLLoaded();	}	else {		trace("error: loading xml");	}};// Populate variables from XML filefunction parseSettingsXML(theFile:XML) {	var rn:XMLNode = theFile.firstChild; // Root node		LOGO_NAME = rn.attributes.logo_name;	LOGO_NAME_COLOR = rn.attributes.logo_name_color;	LOGO_TAGLINE = rn.attributes.logo_tagline;	LOGO_TAGLINE_COLOR = rn.attributes.logo_tagline_color;		if(rn.attributes.use_own_logo == "true") {		USE_OWN_LOGO = true;	}		YOUR_LOGO_FILE = rn.attributes.your_logo_file;	BASIC_TEXT_COLOR = rn.attributes.basic_text_color;	FOOTER_TEXT = rn.attributes.footer_text;	FOOTER_TEXT_COLOR = rn.attributes.footer_text_color;	EMAIL_ADDRESS = rn.attributes.email_address;	EMAIL_TEXT = rn.attributes.email_text;	NAV_COLOR = rn.attributes.nav_color;	NAV_ROLLOVER_COLOR = rn.attributes.nav_rollover_color;	NAV_SELECTED_COLOR = rn.attributes.nav_selected_color;	EMAIL_COLOR = rn.attributes.email_color;	EMAIL_ROLLOVER_COLOR = rn.attributes.email_rollover_color;	BUTTON_COLOR = rn.attributes.button_color;	BUTTON_ROLLOVER_COLOR = rn.attributes.button_rollover_color;	PRELOADER_COLOR = rn.attributes.preloader_color;	BACKGROUND_COLOR = rn.attributes.background_color;	BACKGROUND_GRADIENT_COLOR1 = rn.attributes.background_gradient_color1;	BACKGROUND_GRADIENT_COLOR2 = rn.attributes.background_gradient_color2;	MIDDLE_GRADIENT_COLOR1 = rn.attributes.middle_gradient_color1;	MIDDLE_GRADIENT_COLOR2 = rn.attributes.middle_gradient_color2;	MIDDLE_COLOR = rn.attributes.middle_color;	BORDER_COLOR = rn.attributes.border_color;	THUMB_BACKGROUND_COLOR = rn.attributes.thumb_background_color;	THUMB_BORDER_COLOR = rn.attributes.thumb_border_color;	THUMB_ROLLOVER_COLOR = rn.attributes.thumb_rollover_color;	THUMB_TEXT_COLOR = rn.attributes.thumb_text_color;	THUMB_TEXT_ROLLOVER_COLOR = rn.attributes.thumb_text_rollover_color;	NEW_ICON_COLOR = rn.attributes.new_icon_color;		if(rn.attributes.display_gradient_stripes == "false") {		DISPLAY_GRADIENT_STRIPES = false;	}		DEFAULT_PAGE_TITLE = rn.attributes.default_page_title;	SELECTED_WORK_PAGE_TITLE = rn.attributes.selected_work_page_title;	ABOUT_PAGE_TITLE = rn.attributes.about_page_title;	CONTACT_PAGE_TITLE = rn.attributes.contact_page_title;	RIGHT_CLICK_COPYRIGHT = rn.attributes.right_click_copyright;	PADDING_BOTTOM = rn.attributes.padding_bottom;	}function XMLLoaded():Void {	gotoAndPlay(2);}settings_xml.load("settings.xml");