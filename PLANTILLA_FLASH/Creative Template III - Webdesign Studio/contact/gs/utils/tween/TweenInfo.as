class gs.utils.tween.TweenInfo
{
    var target, property, start, change, name, isPlugin;
    function TweenInfo($target, $property, $start, $change, $name, $isPlugin)
    {
        target = $target;
        property = $property;
        start = $start;
        change = $change;
        name = $name;
        isPlugin = $isPlugin;
    } // End of the function
} // End of Class
